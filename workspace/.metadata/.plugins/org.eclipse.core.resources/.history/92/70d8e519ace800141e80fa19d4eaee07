class Persona (val DNI: String,  val Nombre: String,  val Direccion: String,  val Telefono: Int){
  def Persona (DNI: String,  Nombre: String,  Direccion: String,  Telefono: Int):Persona={
    new Persona(DNI,Nombre,Direccion,Telefono)
  }
  def Agreperson (DNI: String,  Nombre: String,  Direccion: String,  Telefono: Int) : Agreperson
  
}
